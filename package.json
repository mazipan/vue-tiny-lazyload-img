{
  "name": "vue-tiny-lazyload-img",
  "description": "🐌 A small size Vue.js v.2+ directive for lazy loading images",
  "version": "0.0.4",
  "author": "Irfan Maulana <mazipanneh@gmail.com>",
  "license": "MIT",
  "private": false,
  "main": "./dist/VueTinyLazyloadImg.dist.js",
  "keywords": [
    "vue 2 lazy load",
    "vue lazy load",
    "vue lazyload",
    "vue lazyload image"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/mazipan/vue-tiny-lazyload-img.git"
  },
  "bugs": {
    "url": "https://github.com/mazipan/vue-tiny-lazyload-img/issues"
  },
  "engines": {
    "node": ">= 6.0.0",
    "npm": ">= 5.0.0"
  },
  "files": [
    "dist/VueTinyLazyloadImg.dist.js",
    "dist/VueTinyLazyloadImg.dist.js.gz",
    "dist/VueTinyLazyloadImg.dist.js.map"
  ],
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server --hot --color --open --config webpack.config.dev.js",
    "build": "cross-env NODE_ENV=production webpack --progress --hide-modules --config webpack.config.dev.js",
    "pack": "cross-env NODE_ENV=production webpack --config webpack.config.js",
    "dist": "npm run build && npm run pack",
    "test": "./node_modules/.bin/jest --coverage",
    "deploy": "node ./gh-publish.js"
  },
  "dependencies": {
    "vue": "^2.5.16"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "devDependencies": {
    "autoprefixer": "^8.2.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.6.1",
    "clean-webpack-plugin": "0.1.19",
    "compression-webpack-plugin": "^1.1.11",
    "copy-webpack-plugin": "^4.5.1",
    "cross-env": "^5.1.4",
    "css-loader": "^0.28.11",
    "es6-promise": "^4.2.4",
    "eslint": "^4.19.1",
    "eslint-plugin-vue": "4.4.0",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "^1.1.11",
    "gh-pages": "1.1.0",
    "html-webpack-inline-chunk-plugin": "1.1.1",
    "html-webpack-plugin": "^3.2.0",
    "intersection-observer": "^0.5.0",
    "jest": "^22.4.3",
    "node-sass": "^4.8.3",
    "optimize-css-assets-webpack-plugin": "3.2.0",
    "postcss-import": "^11.1.0",
    "postcss-loader": "^2.1.3",
    "postcss-url": "^7.3.2",
    "raw-loader": "^0.5.1",
    "sass-loader": "^6.0.7",
    "source-map-loader": "^0.2.3",
    "style-loader": "^0.20.3",
    "svg-url-loader": "2.3.2",
    "url-loader": "1.0.1",
    "vue-google-adsense": "^1.2.0",
    "vue-jest": "^2.4.1",
    "vue-loader": "^14.2.2",
    "vue-script2": "^2.0.2",
    "vue-template-compiler": "^2.5.16",
    "vue-test-utils": "^1.0.0-beta.11",
    "webpack": "^3.11.0",
    "webpack-dev-server": "^2.11.2"
  },
  "eslintIgnore": [
    "test/*.js",
    "dist/*.js",
    "node_modules"
  ],
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "plugin:vue/recommended"
    ]
  },
  "jest": {
    "mapCoverage": true,
    "coverageDirectory": "test/coverage",
    "moduleNameMapper": {
      "^vue$": "vue/dist/vue.common.js"
    },
    "moduleFileExtensions": [
      "js",
      "vue"
    ],
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest",
      ".*\\.(vue)$": "<rootDir>/node_modules/vue-jest"
    }
  }
}
